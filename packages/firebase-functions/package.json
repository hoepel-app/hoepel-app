    "firebase-functions-test": "^0.2.1",
    "husky": "^4.2.3",
    "lerna": "^3.18.4",
    "lint-staged": "^10.0.7",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "engines": {
    "node": "10"
  }
