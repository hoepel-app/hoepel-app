{
  "name": "@hoepel.app/firebase-functions",
  "description": "Serverless functions for hoepel.app implemented as Firebase functions",
  "version": "0.3.12",
  "main": "dist/index.js",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build": "tsc && yarn build",
    "serve": "yarn build && GOOGLE_APPLICATION_CREDENTIALS=\"$PWD/key.json\" firebase serve --only functions",
    "shell": "yarn build && GOOGLE_APPLICATION_CREDENTIALS=\"$PWD/key.json\" firebase functions:shell",
    "start": "yarn shell",
    "logs": "firebase functions:log",
    "bundle": "./create-bundle.sh"
  },
  "private": true,
  "dependencies": {
    "@backupfire/firebase": "^0.13.0",
    "@hoepel.app/cloud-build-notifier": "^0.3.12",
    "@hoepel.app/domain": "^0.3.12",
    "@hoepel.app/export-docx": "^0.3.12",
    "@hoepel.app/export-xlsx": "^0.3.12",
    "@hoepel.app/firestore-events": "^0.3.12",
    "@hoepel.app/old-events": "^0.3.12",
    "@hoepel.app/types": "^1.9.0",
    "@sentry/node": "^5.10.1",
    "apollo-server-express": "^2.10.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase-admin": "^8.6.0",
    "firebase-functions": "^3.3.0",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
